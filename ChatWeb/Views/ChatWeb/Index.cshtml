@{
   Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title></title>

</head>
<body>
    
    <div class="container">
        <h3 class=" text-center">Chat</h3>

        <div class="row">
            <div class="col-xs-12 col-sm-6">
                <label>from:</label>
                <input id="dateFrom" type="text" />
            </div>
        </div>

        <div class="row">
            <div class="col-xs-12 col-sm-6">
                <label>&nbsp &nbsp to:</label>
                <input id="dateTo" type="text" />
            </div>
        </div>

        <div class="messaging">
            <div class="inbox_msg">
                <div class="inbox_people">
                    <div class="headind_srch">
                        <div class="recent_heading">
                            <h4 id="currentUser">Users</h4>
                        </div>
                        <div class="srch_bar">
                            <div class="stylish-input-group">

                            </div>
                        </div>
                    </div>
                    <div id="user_Content" class="inbox_chat">


                    </div>
                </div>

                <div class="mesgs">
                    <div class="msg_history">

                    </div>
                    <div class="type_msg">
                        <div class="input_msg_write">
                            <input id="message" type="text" class="write_msg" placeholder="Type a message" />
                            <button id="sendmessage" class="msg_send_btn" type="button">
                            </button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>


    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Chat.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <!-- SignalR -->
    <script src="~/signalr/hubs"></script>
    <script src="~/Scripts/Chat/ChatHandler.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
   
</body>
</html>
